{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/imlolman/PayPal-PHP-SDK.git"
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^2.3",
        "cweagans/composer-patches": "^1.6",
        "drupal/admin_toolbar": "^3",
        "drupal/blazy": "^2.0",
        "drupal/chosen": "^3.0",
        "drupal/classy": "^1.0",
        "drupal/config_filter": "^2.0",
        "drupal/config_ignore": "^2.0",
        "drupal/contact_storage": "^1.0",
        "drupal/core-composer-scaffold": "^10.1",
        "drupal/core-project-message": "^10.1",
        "drupal/core-recommended": "^10.1",
        "drupal/core-vendor-hardening": "^10.1",
        "drupal/email_registration": "^1.0",
        "drupal/facets": "^3",
        "drupal/field_group": "^3.0",
        "drupal/field_permissions": "^1.0",
        "drupal/google_analytics": "^4",
        "drupal/honeypot": "^2.0",
        "drupal/jquery_ui_accordion": "^2.1",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/page_manager": "^4.0",
        "drupal/pathauto": "^1",
        "drupal/redis": "^1.5",
        "drupal/search_api": "^1.14",
        "drupal/slick": "^2.9",
        "drupal/slick_views": "^2.7",
        "drupal/stage_file_proxy": "^2.1",
        "drupal/swiftmailer": "^2.0",
        "drupal/symfony_mailer_lite": "^2.0",
        "drupal/video_embed_field": "^2.0",
        "drush/drush": "^12",
        "paypal/rest-api-sdk-php": "dev-master",
        "vlucas/phpdotenv": "^5.4"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "drupal/core-vendor-hardening": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "enable-patching": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "patches": {
            "drupal/core": {
                "Syslog error": "patches/3333215-22.patch"
            },
            "paypal/rest-api-sdk-php": {
                "Compatibility": "patches/paypal-compatibility.patch"
            }
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/sites/development.services.yml": false
            }
        }
    },
    "scripts": {
    },
    "require-dev": {
        "drupal/core-dev": "^10.1",
        "drupal/upgrade_status": "*"
    }
}
